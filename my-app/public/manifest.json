{
  "manifest_version": 3,
  "name": "GreenChoice",
  "version": "0.1.0",
  "description": "DetecteazÄƒ alternative eco pe paginile de produse.",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/content.js"],
      "css": ["content/content.css"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injected/mount.js"],
      "matches": ["<all_urls>"]
    }
  ]
}